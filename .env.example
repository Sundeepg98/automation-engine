# Automation Engine Configuration
# Copy to .env and fill in your values

# Google Cloud Project
PROJECT_ID=automation-engine-463103
PROJECT_NUMBER=YOUR_PROJECT_NUMBER

# Service Account
SERVICE_ACCOUNT_EMAIL=automation-service@automation-engine-463103.iam.gserviceaccount.com
SERVICE_ACCOUNT_KEY_PATH=./credentials/automation-service-key.json

# Google APIs Configuration
GOOGLE_APPLICATION_CREDENTIALS=./credentials/automation-service-key.json

# API Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_VERSION=v1
ENABLE_CORS=true

# Execution Configuration
MAX_CONCURRENT_EXECUTIONS=10
DEFAULT_TIMEOUT_SECONDS=300
MAX_TIMEOUT_SECONDS=3600

# Plugin Configuration
ENABLED_PLUGINS=colab,cloudrun,vertexai,storage
PLUGIN_DISCOVERY_PATH=./src/plugins

# Security
ENABLE_API_KEYS=true
API_KEY_HEADER=X-API-Key
JWT_SECRET=your-secret-here
RATE_LIMIT_PER_MINUTE=60

# Monitoring
LOG_LEVEL=INFO
ENABLE_METRICS=true
METRICS_PORT=9090
ENABLE_TRACING=false

# Cloud Resources
CLOUD_STORAGE_BUCKET=automation-engine-data
FIRESTORE_DATABASE=(default)
BIGQUERY_DATASET=automation_analytics

# Multi-tenant Configuration
ENABLE_MULTI_TENANT=true
MAX_TENANTS=100
TENANT_ISOLATION_MODE=namespace

# Feature Flags
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_GPU_SUPPORT=false
ENABLE_BATCH_OPERATIONS=true